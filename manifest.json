{
  "manifest_version": 2,
  "version": "0.7.7",
  "name": "KICK/RUR Ticker",
  "description": "KICK/RUR last price ticker for Yobit.net - chrome extension",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html",
    "default_title":"KICK/RUR Ticker"
  },
  "background": {
    "scripts": [

      "js/modules/rgb.js",

      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": ["js/jquery.min.js", "js/content.js"],
      "matches": ["*://*/*"],
      "run_at": "document_start"
    }
  ]
}